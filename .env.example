# Copy to .env and adjust as needed.

# ===== Postgres =====
POSTGRES_USER=tsi
POSTGRES_PASSWORD=tsi
POSTGRES_DB=tsi
POSTGRES_PORT=5432

# ===== Streamlit app =====
TSI_PORT=8501

# Optional Postgres pool tuning (Rust backend)
PG_POOL_MAX=10
PG_POOL_MIN=1
PG_CONN_TIMEOUT_SEC=30
PG_IDLE_TIMEOUT_SEC=600
PG_MAX_RETRIES=3
PG_RETRY_DELAY_MS=100

# Rust logging
RUST_LOG=info

